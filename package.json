{
  "name": "ng-perfect-scrollbar",
  "version": "0.0.0",
  "scripts": {
    "ng": "node ./node_modules/@angular/cli/bin/ng",
    "start": "npm run ng -- serve",
    "build": "npm run ng -- build",
    "watch": "npm run ng -- build --watch --configuration development",
    "build:lib": "npm run ng -- build @harvest-dev/ng-perfect-scrollbar --configuration production && cp *.md dist/harvest/ng-perfect-scrollbar",
    "build:demo": "npm run ng -- build ng-perfect-scrollbar --configuration production",
    "publish:lib": "cd dist/harvest/ng-perfect-scrollbar && npm publish ",
    "test": "npm run ng -- test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.5",
    "@angular/common": "^18.0.5",
    "@angular/compiler": "^18.0.5",
    "@angular/core": "^18.0.5",
    "@angular/forms": "^18.0.5",
    "@angular/platform-browser": "^18.0.5",
    "@angular/platform-browser-dynamic": "^18.0.5",
    "@angular/router": "^18.0.5",
    "perfect-scrollbar": "^1.5.5",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.3",
    "zone.js": "~0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.6",
    "@angular/cli": "~18.0.6",
    "@angular/compiler-cli": "^18.0.5",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^18.0.0",
    "typescript": "~5.4.5"
  },
  "engines": {
    "node": "20.14.0",
    "npm": "10.7.0"
  }
}
