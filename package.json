{
  "name": "ng-perfect-scrollbar",
  "version": "0.0.0",
  "scripts": {
    "ng": "node ./node_modules/@angular/cli/bin/ng",
    "start": "npm run ng -- serve",
    "build": "npm run ng -- build",
    "watch": "npm run ng -- build --watch --configuration development",
    "build:lib": "npm run ng -- build @harvest-dev/ng-perfect-scrollbar --configuration production && cp *.md dist/harvest/ng-perfect-scrollbar",
    "build:demo": "npm run ng -- build ng-perfect-scrollbar --configuration production",
    "publish:lib": "cd dist/harvest/ng-perfect-scrollbar && npm publish ",
    "test": "npm run ng -- test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "perfect-scrollbar": "^1.5.5",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular/cli": "~16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^16.2.0",
    "typescript": "~5.1.3"
  },
  "engines": {
    "node": "20.5.0",
    "npm": "9.8.0"
  }
}
